module tester {
	namespace "http://confvillain.mellon-collie.net/tester/";
	prefix tester;

  leaf simplestleaf {
    type string;
  }
  
  leaf simpleoper {
    type string;
    config false;
  }

  container tupperware {

    leaf oper {
      type string;
      config false;
    }

    leaf config {
      type string;
    }
  }

  list simplelist {
    key id;

    leaf id {
      type string;
    }

    leaf val {
      type string;
    }

    leaf operval {
      type string;
      config false;
    }
  }

}
